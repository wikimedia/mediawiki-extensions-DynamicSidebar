{
	"name": "DynamicSidebar",
	"version": "1.1",
	"author": "Ryan Lane",
	"url": "https://www.mediawiki.org/wiki/Extension:DynamicSidebar",
	"descriptionmsg": "dynamicsidebar-desc",
	"license-name": "GPL-2.0-or-later",
	"type": "other",
	"config": {
		"DynamicSidebarUseCategories": {
			"value": true
		},
		"DynamicSidebarUseGroups": {
			"value": true
		},
		"DynamicSidebarUsePageCategories": {
			"value": false
		},
		"DynamicSidebarUseUserpages": {
			"value": true
		}
	},
	"Hooks": {
		"SidebarBeforeOutput": "DynamicSidebar::modifySidebar"
	},
	"MessagesDirs": {
		"DynamicSidebar": [
			"i18n"
		]
	},
	"AutoloadClasses": {
		"DynamicSidebar": "includes/DynamicSidebar.php"
	},
	"manifest_version": 2,
	"requires": {
		"MediaWiki": ">= 1.29.0"
	}
}
